{
  "manifest_version": 2,

  "name": "QQZone Dynamic Emotion",
  "description": "Turn the static emotions in QQZone to dynamic",
  "version": "1.0.2",

  "content_scripts": [
    {
      "matches": ["http://*.qzone.qq.com/*"],
      "js": ["replace.js"]
    }
  ],
  "icons": 
  { 
    "16": "icon16.png",             
    "48": "icon48.png",            
    "128": "icon128.png" 
  },  
  "browser_action": {
    "default_icon": "icon.png"
  }
}
