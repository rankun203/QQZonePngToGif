{
  "manifest_version": 2,

  "name": "QQZone Dynamic Emotion",
  "description": "Turn the static emotions in QQZone to dynamic",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://*.qzone.qq.com/*"],
      "js": ["replace.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png"
  }
}
